{
    "name": "yidash",
    "type": "module",
    "version": "1.4.0",
    "private": false,
    "description": "yidash（易大师）一万个JavaScript实用方法库",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "author": "chensuiyi <bimostyle@qq.com>",
    "homepage": "https://yidash.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo/yidash"
    },
    "keywords": [
        "lodash",
        "utils",
        "helper",
        "help"
    ],
    "exports": {
        ".": "./index.js",
        "./yidashLibNames.js": "./yidashLibNames.js"
    },
    "files": [
        "config/",
        "lib/",
        "index.js",
        "LICENSE",
        "package.json",
        "README.md",
        "yidashLibNames.js"
    ],
    "scripts": {
        "gen": "node generate.js",
        "docs": "del-cli docs & jsdoc -c jsdoc.json",
        "test": "vitest --dir lib",
        "test-ui": "vitest --ui --dir lib",
        "lint": "eslint ./lib",
        "lint:fix": "eslint ./lib --fix",
        "format": "prettier --write .",
        "inspect": "eslint --inspect-config"
    },
    "dependencies": {
        "date-fns": "^3.6.0",
        "dayjs": "^1.11.12"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^0.5.2",
        "@eslint/js": "^9.8.0",
        "@vitest/ui": "^2.0.5",
        "colorette": "^2.0.20",
        "del-cli": "^5.1.0",
        "docdash-extended-chensuiyi": "^1.1.1",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsdoc": "^48.11.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-regexp": "^2.6.0",
        "fs-extra": "^11.2.0",
        "globals": "^15.9.0",
        "jsdoc": "^4.0.3",
        "lint-staged": "^15.2.7",
        "log-symbols": "^6.0.0",
        "prettier": "^3.3.3",
        "simple-git-hooks": "^2.11.1",
        "vitest": "^2.0.5"
    },
    "simple-git-hooks": {
        "pre-commit": "pnpm exec lint-staged --concurrent false"
    },
    "lint-staged": {
        "*": [
            "prettier --write --cache --ignore-unknown"
        ]
    }
}
